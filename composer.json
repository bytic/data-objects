{
  "name": "bytic/data-transfer-object",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/bytic/data-transfer-object",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\DataTransferObject\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\DataTransferObject\\Tests\\": "tests/src",
      "ByTIC\\DataTransferObject\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.1",
    "bytic/inflector": "^1.0",
    "bytic/utility": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0",
      "mockery/mockery": "^1.0",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  }
}
