{
    "name": "bytic/data-objects",
    "type": "library",
    "description": "Data objects for with property mutators and casting",
    "homepage": "https://github.com/bytic/data-objects",
    "license": "MIT",
    "authors": [
        {
            "name": "Gabriel Solomon",
            "email": "hello@gabrielsolomon.ro"
        }
    ],
    "autoload": {
        "psr-4": {
            "ByTIC\\DataObjects\\": "src"
        },
        "classmap": [
            "legacy"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ByTIC\\DataObjects\\Tests\\": "tests/src",
            "ByTIC\\DataObjects\\Tests\\Fixtures\\": "tests/fixtures"
        }
    },
    "require": {
        "php": "^7.1",
        "bytic/inflector": "^1.0",
        "bytic/utility": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "mockery/mockery": "^1.0",
        "phpbench/phpbench": "^0.17",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
