{
    "name": "bytic/data-objects",
    "description": "Data objects for with property mutators and casting",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Gabriel Solomon",
            "email": "hello@gabrielsolomon.ro"
        }
    ],
    "homepage": "https://github.com/bytic/data-objects",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-simplexml": "*",
        "bytic/inflector": "^1.0",
        "bytic/utility": "^1.0.42"
    },
    "require-dev": {
        "bytic/phpqatools": "^1.0",
        "mockery/mockery": "^1.0",
        "phpbench/phpbench": "^1.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "ByTIC\\DataObjects\\": "src",
            "ByTIC\\DataObjects\\Legacy\\": "legacy"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ByTIC\\DataObjects\\Tests\\": "tests/src",
            "ByTIC\\DataObjects\\Tests\\Fixtures\\": "tests/fixtures"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "ergebnis/composer-normalize": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "bytic/phpqatools": true
        }
    }
}
